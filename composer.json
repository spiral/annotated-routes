{
    "name": "spiral/annotated-routes",
    "type": "library",
    "description": "Annotated routes for Spiral Framework",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/annotated-routes"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "spiral/router": "^2.8",
        "spiral/annotations": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "^1.3",
        "laminas/laminas-diactoros": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Router\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.8.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
