{
    "name": "spiral/annotated-routes",
    "type": "library",
    "description": "Annotated routes for Spiral Framework",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/annotated-routes"
    },
    "authors": [
        {
            "name": "Wolfy-J",
            "email": "wolfy.jd@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "spiral/router": "^2.7",
        "spiral/annotations": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "^1.3",
        "laminas/laminas-diactoros": "^2.3",
        "spiral/framework": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Router\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Router\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
